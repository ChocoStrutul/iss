<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="HBNEntities">

    <class name="model.Log" table="logs">
        <id name="id" column="id" type="java.lang.Long">
            <generator class="increment"/>
        </id>
        <property name="arrivalTime" type="string" column="arrivalTime"/>
        <property name="uid" type="java.lang.Long" column="uid"/>
    </class>

</hibernate-mapping>
